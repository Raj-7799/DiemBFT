import sys

class Replica(process):
    def setup(pID:int, other_replicas, faulty_replica_count: int, replica_count:int, messages:int, delta:int):
        self.pID = pID
        self.replicas = other_replicas
        self.fCount = faulty_replica_count
        self.rCount = replica_count
        self.messages = messages
        self.tDelta = delta
    
    def run():
        output("Yo Starting replica with pID", self.pID)
